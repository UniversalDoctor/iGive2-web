{
  "extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": ["jsx-a11y", "prettier"],

  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/jsx-props-no-spreading": 0,
    "react/forbid-prop-types": 0,
    "prettier/prettier": ["error"]
  },
  "env": {
    "browser": true
  },
  "globals": {
    "DOMParser": true,
    "document": true,
    "fetch": true,
    "navigator": true,
    "window": true
  },
  "ignorePatterns": [
    "build",
    "node_modules/",
    ".DS_Store"
  ]
}